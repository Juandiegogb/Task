<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Panel de control</title>
    <link rel="stylesheet" href="test.css" />
  </head>
  <body>
    <h1>Panel de control</h1>

    <ul>
      <% datos.forEach(function(dato) { %>
      <li>
        <span><%= dato.odt %></span>
        <span><%= dato.cliente %></span>
        <span><%= dato.grupo %></span>
        <span><%= dato.clasificacion %></span>
        <span><%= dato.cantidad %></span>
        <span><%= dato.rem %></span>
        <span><%= dato.observaciones %></span>
        <span><%= dato.informe %></span>
        <span><%= dato.precio %></span>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
